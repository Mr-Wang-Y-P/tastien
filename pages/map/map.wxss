/* pages/map/map.wxss */
.page_bd {
  padding: 30rpx;
}

.bw_mapController__hd {
  display: flex;
  align-items: center;
}

.bw_mapController__title {
  padding-left: 54rpx;
  position: relative;
}

/*伪元素 css 声明 没有标签 可以页面显示*/
.bw_mapController__title::before {
  content: "";
  /*伪元素必要属性*/
  position: absolute;
  top: 0;
  left: 0;
  width: 54rpx;
  height: 54rpx;
  background: url(data:image/png;base64,);
  background-size: 80%;
  background-repeat: no-repeat;
}

.bw_mapController__bd {
  height: 300rpx;
  position: relative;
  transition: height 0.5s;
  overflow: hidden;
  margin-top: 20rpx;
}

.bw_mapController__bd map {
  width: 100%;
  height: 100%;
}

.bw_mapController__bd .bw_mapController__switch {
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  height: 54rpx;
  background-color: #fff;
  text-align: center;
}

.bw_mapController__bd-hide {
  height: 0;
}